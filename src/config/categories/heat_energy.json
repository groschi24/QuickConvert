{
  "temperature": {
    "label": "Temperature",
    "baseUnit": "kelvin",
    "units": {
      "kelvin": {
        "label": "K",
        "conversion": { "baseUnit": "kelvin", "formula": "x=x" }
      },
      "celsius": {
        "label": "°C",
        "conversion": {
          "baseUnit": "kelvin",
          "formula": "x=x+273.15",
          "reverseFormula": "x=x-273.15"
        }
      },
      "fahrenheit": {
        "label": "°F",
        "conversion": {
          "baseUnit": "kelvin",
          "formula": "x=(x-32)*5/9+273.15",
          "reverseFormula": "x=(x-273.15)*9/5+32"
        }
      },
      "rankine": {
        "label": "°R",
        "conversion": {
          "baseUnit": "kelvin",
          "formula": "x=x*0.555556",
          "reverseFormula": "x=x*1.8"
        }
      }
    }
  },
  "thermal_conductivity": {
    "label": "Thermal Conductivity",
    "baseUnit": "watts_per_meter_kelvin",
    "units": {
      "watts_per_meter_kelvin": {
        "label": "W/m·K",
        "conversion": { "baseUnit": "watts_per_meter_kelvin", "formula": "x=x" }
      },
      "btu_per_hour_foot_fahrenheit": {
        "label": "BTU/h·ft·°F",
        "conversion": {
          "baseUnit": "watts_per_meter_kelvin",
          "formula": "x=x*1.730735"
        }
      },
      "kilocalories_per_hour_meter_celsius": {
        "label": "kcal/h·m·°C",
        "conversion": {
          "baseUnit": "watts_per_meter_kelvin",
          "formula": "x=x*0.860421"
        }
      }
    }
  },
  "energy": {
    "label": "Energy",
    "baseUnit": "joules",
    "units": {
      "joules": {
        "label": "J",
        "conversion": { "baseUnit": "joules", "formula": "x=x" }
      },
      "kilojoules": {
        "label": "kJ",
        "conversion": { "baseUnit": "joules", "formula": "x=x*1000" }
      },
      "calories": {
        "label": "cal",
        "conversion": { "baseUnit": "joules", "formula": "x=x*4.184" }
      },
      "kilowatt_hours": {
        "label": "kWh",
        "conversion": { "baseUnit": "joules", "formula": "x=x*3600000" }
      },
      "btu": {
        "label": "BTU",
        "conversion": { "baseUnit": "joules", "formula": "x=x*1055.06" }
      }
    }
  },
  "fuel_efficiency": {
    "label": "Fuel Efficiency",
    "baseUnit": "grams_per_kilowatt_hour",
    "units": {
      "grams_per_kilowatt_hour": {
        "label": "g/kWh",
        "conversion": {
          "baseUnit": "grams_per_kilowatt_hour",
          "formula": "x=x"
        }
      },
      "pounds_per_horsepower_hour": {
        "label": "lb/hp·h",
        "conversion": {
          "baseUnit": "grams_per_kilowatt_hour",
          "formula": "x=x*608.277"
        }
      },
      "kilograms_per_megajoule": {
        "label": "kg/MJ",
        "conversion": {
          "baseUnit": "grams_per_kilowatt_hour",
          "formula": "x=x*0.0036"
        }
      }
    }
  },
  "fuel_consumption": {
    "label": "Fuel Consumption",
    "baseUnit": "liters_per_100km",
    "units": {
      "liters_per_100km": {
        "label": "L/100km",
        "conversion": { "baseUnit": "liters_per_100km", "formula": "x=x" }
      },
      "miles_per_gallon": {
        "label": "MPG",
        "conversion": {
          "baseUnit": "liters_per_100km",
          "formula": "x=235.215/x"
        }
      },
      "kilometers_per_liter": {
        "label": "km/L",
        "conversion": { "baseUnit": "liters_per_100km", "formula": "x=100/x" }
      },
      "gallons_per_100_miles": {
        "label": "gal/100mi",
        "conversion": { "baseUnit": "liters_per_100km", "formula": "x=1000/x" }
      }
    }
  }
}
